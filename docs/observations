from fix_encoding import FixEncoding
fixEncoding = FixEncoding(str)
fixEncoding.is_invalid()
fixEncoding.fix()

fix_encoding (-v)
lists cwd() and verifies files

fix_encoding (-f)
lists cwd() and fixes all files

fix_encoding (-f -v | -fv)
lists cwd() and fixes all files

fix_encoding (-v, valid_path)
if file: verifies file_name
if dir: verifies dir_name

fix_encoding (-f, valid_path)
if file: fixes file_name
if dir: fixes dir_name

fix_encoding (-f -v|-fv, valid_path)
if file: fixes file_name
if dir: fixes dir_name




